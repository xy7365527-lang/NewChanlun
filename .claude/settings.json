{
  "alwaysThinkingEnabled": false,
  "env": {
    "CLAUDE_CODE_EXPERIMENTAL_AGENT_TEAMS": "1",
    "ENABLE_TOOL_SEARCH": "true",
    "DISABLE_INTERLEAVED_THINKING": "1",
    "MAX_THINKING_TOKENS": "0"
  },
  "hooks": {
    "SessionStart": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": ".claude/hooks/session-start-ceremony.sh"
          }
        ]
      }
    ],
    "PreCompact": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": ".claude/hooks/precompact-save.sh"
          }
        ]
      }
    ],
    "PreToolUse": [
      {
        "matcher": "Task",
        "hooks": [
          {
            "type": "command",
            "command": ".claude/hooks/ceremony-guard.sh"
          },
          {
            "type": "command",
            "command": ".claude/hooks/agent-team-enforce.sh"
          }
        ]
      },
      {
        "matcher": "Bash",
        "hooks": [
          {
            "type": "command",
            "command": ".claude/hooks/double-helix-verify.sh"
          }
        ]
      },
      {
        "matcher": "Write",
        "hooks": [
          {
            "type": "command",
            "command": ".claude/hooks/definition-write-guard.sh"
          },
          {
            "type": "command",
            "command": ".claude/hooks/genealogy-write-guard.sh"
          },
          {
            "type": "command",
            "command": ".claude/hooks/spec-write-guard.sh"
          },
          {
            "type": "command",
            "command": ".claude/hooks/hub-node-impact-guard.sh"
          }
        ]
      },
      {
        "matcher": "Edit",
        "hooks": [
          {
            "type": "command",
            "command": ".claude/hooks/definition-write-guard.sh"
          },
          {
            "type": "command",
            "command": ".claude/hooks/genealogy-write-guard.sh"
          },
          {
            "type": "command",
            "command": ".claude/hooks/spec-write-guard.sh"
          },
          {
            "type": "command",
            "command": ".claude/hooks/hub-node-impact-guard.sh"
          }
        ]
      }
    ],
    "Stop": [
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": ".claude/hooks/ceremony-completion-guard.sh"
          }
        ]
      },
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": ".claude/hooks/post-session-pattern-detect.sh"
          }
        ]
      },
      {
        "matcher": "",
        "hooks": [
          {
            "type": "command",
            "command": ".claude/hooks/meta-observer-guard.sh"
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": "TaskUpdate",
        "hooks": [
          {
            "type": "command",
            "command": ".claude/hooks/function-length-guard.sh"
          }
        ]
      },
      {
        "matcher": "Bash",
        "hooks": [
          {
            "type": "command",
            "command": ".claude/hooks/flow-continuity-guard.sh"
          }
        ]
      },
      {
        "matcher": "Bash",
        "hooks": [
          {
            "type": "command",
            "command": ".claude/hooks/crystallization-guard.sh"
          }
        ]
      },
      {
        "matcher": "Bash",
        "hooks": [
          {
            "type": "command",
            "command": ".claude/hooks/topology-guard.sh"
          }
        ]
      },
      {
        "matcher": "Write",
        "hooks": [
          {
            "type": "command",
            "command": ".claude/hooks/result-package-guard.sh"
          },
          {
            "type": "command",
            "command": ".claude/hooks/downstream-action-guard.sh"
          },
          {
            "type": "command",
            "command": ".claude/hooks/dag-validation-guard.sh"
          },
          {
            "type": "command",
            "command": ".claude/hooks/source-auditor-prompt.sh"
          },
          {
            "type": "command",
            "command": ".claude/hooks/genealogy-gemini-verify.sh"
          }
        ]
      },
      {
        "matcher": "Edit",
        "hooks": [
          {
            "type": "command",
            "command": ".claude/hooks/result-package-guard.sh"
          },
          {
            "type": "command",
            "command": ".claude/hooks/downstream-action-guard.sh"
          },
          {
            "type": "command",
            "command": ".claude/hooks/dag-validation-guard.sh"
          },
          {
            "type": "command",
            "command": ".claude/hooks/source-auditor-prompt.sh"
          },
          {
            "type": "command",
            "command": ".claude/hooks/genealogy-gemini-verify.sh"
          }
        ]
      },
      {
        "matcher": "TeamCreate",
        "hooks": [
          {
            "type": "command",
            "command": ".claude/hooks/team-structural-inject.sh"
          }
        ]
      }
    ]
  },
  "enabledPlugins": {
    "frontend-design@claude-plugins-official": true,
    "context7@claude-plugins-official": true,
    "code-review@claude-plugins-official": true,
    "code-simplifier@claude-plugins-official": true,
    "superpowers@claude-plugins-official": true,
    "pr-review-toolkit@claude-plugins-official": true,
    "github@claude-plugins-official": true,
    "feature-dev@claude-plugins-official": true,
    "ralph-loop@claude-plugins-official": true,
    "serena@claude-plugins-official": true,
    "figma@claude-plugins-official": true,
    "explanatory-output-style@claude-plugins-official": true,
    "greptile@claude-plugins-official": true,
    "Notion@claude-plugins-official": true,
    "claude-md-management@claude-plugins-official": true,
    "plugin-dev@claude-plugins-official": true,
    "atlassian@claude-plugins-official": true,
    "agent-sdk-dev@claude-plugins-official": true,
    "pyright-lsp@claude-plugins-official": true,
    "supabase@claude-plugins-official": true,
    "security-guidance@claude-plugins-official": true,
    "commit-commands@claude-plugins-official": true,
    "playwright@claude-plugins-official": true,
    "typescript-lsp@claude-plugins-official": true,
    "claude-code-setup@claude-plugins-official": true
  },
  "autoUpdatesChannel": "latest",
  "permissions": {
    "allow": [
      "Read",
      "Edit",
      "MultiEdit",
      "Write",
      "Grep",
      "Glob",
      "Task",
      "TodoWrite",
      "Bash(*)",
      "mcp__plugin_serena_serena__*",
      "SendMessage",
      "TaskCreate",
      "TaskUpdate",
      "TaskList",
      "TaskGet",
      "WebFetch",
      "WebSearch",
      "ToolSearch",
      "Skill",
      "NotebookEdit"
    ],
    "deny": [
      "Bash(rm -rf *)",
      "Bash(curl *)",
      "Bash(wget *)",
      "Bash(git push --force*)",
      "Bash(git reset --hard*)"
    ]
  }
}
