# Alpha Vantage API 密钥
ALPHAVANTAGE_API_KEY=your_api_key_here

# DataBento API 密钥
DATABENTO_API_KEY=your_databento_api_key_here

# Google API
GOOGLE_API_KEY=your_google_api_key_here

# 本地缓存目录
CACHE_DIR=.cache

# 规格断言（调试/测试用）
# 设为 1/true/yes/on 时，A 系统断言失败将直接抛错（用于“锁语义”）
NEWCHAN_ASSERT=0

# IBKR 连接配置
# TWS paper: 7497, TWS live: 7496
# Gateway paper: 4002, Gateway live: 4001
IB_HOST=127.0.0.1
IB_PORT=7497
IB_CLIENT_ID=1
